  "homepage": "https://github.com/ErickEspinozaT/TypeScriptRestAPI#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/ip": "^1.1.0",
    "@types/morgan": "^1.9.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
