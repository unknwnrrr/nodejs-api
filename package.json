    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.17.1",
    "import-sort-style-module": "^6.0.0",
    "ip": "^1.1.5",
    "morgan": "^1.10.0",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6"
  },
  "importSort": {
